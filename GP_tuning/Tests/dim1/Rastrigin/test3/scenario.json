{
    "application_name": "GP_Rastrigin",
    "models": {
        "model": "gaussian_process",
        "acquisition_function": "EI",
        "local_search_starting_points": 10,
        "local_search_random_points": 1000,
        "log_transform_output": true,
        "lengthscale_prior": {
            "name": "gamma",
            "parameters": [
                1.3,
                0.1
            ]
        },
        "epsilon_greedy_threshold": 1,
        "predict_noiseless": false,
        "exploration_augmentation": 0.01
    },
    "optimization_objectives": [
        "value"
    ],
    "optimization_iterations": 1,
    "time_budget": 7,
    "normalize_inputs": true,
    "output_data_file": "Tests/dim1/Rastrigin/test3/output_samples_1.csv",
    "resume_optimization": false,
    "resume_optimization_data": "Tests/dim1/Rastrigin/test3/output_samples_1.csv",
    "design_of_experiment": {
        "number_of_samples": 2
    },
    "input_parameters": {
        "x1": {
            "parameter_type": "real",
            "values": [
                -5,
                5
            ]
        }
    }
}